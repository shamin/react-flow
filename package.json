{
  "name": "@space-kit/react-flow",
  "version": "0.0.7",
  "description": "Create flow charts in react",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "webpack-dev-server --open --hot --mode development ",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build": "npm run build:cjs && npm run build:esm",
    "lint": "eslint \"{src,src-docs}/**/*.{ts,tsx,js}\" --max-warnings 0 --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shamin/react-flow.git"
  },
  "keywords": [
    "flow chart",
    "flow",
    "react"
  ],
  "license": "MIT",
  "author": "Shamin",
  "bugs": {
    "url": "https://github.com/shamin/react-flow/issues"
  },
  "dependencies": {
    "@space-kit/portal": "0.1.0-alpha.1",
    "immer": "^8.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-jest": "^26.5.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.5.2",
    "prettier": "^2.1.2",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "webpack": "^5.0.0-rc.6",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/shamin/react-flow#readme"
